includes:
    - vendor/larastan/larastan/extension.neon
    - phpstan-baseline.neon

parameters:
    level: 9
    paths:
        - app
        - config
        - database
        - routes
    
    
    # Ignore patterns
    excludePaths:
        - archive/*
        - vendor/*
        - storage/*
        - bootstrap/cache/*
    
    # Laravel specific
    checkModelProperties: true
    checkOctaneCompatibility: true